// Babel 配置
{
  "presets": [
    "@babel/preset-env", // 解析 ES6+ 的语法, 解析成 ES5
    "@babel/preset-react", //解析 React 的语法
    [
      "@babel/preset-typescript",
      {
        "onlyRemoveTypeImports": true
      }
    ]
  ],
  "plugins": [
    // 解决多个地方使用相同代码导致打包重复的问题
    [
      "@babel/plugin-transform-runtime"
    ]
  ],
  "ignore": [
    "node_modules/**"
  ]
}
