{
  "name": "daphnis-hooks",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "eslint -c ./.eslintrc --ext .jsx,.js,.ts,.tsx src && stylelint ./src",
    "lint-fix": "eslint -c ./.eslintrc --ext .jsx,.js,.ts,.tsx src --fix && stylelint ./src --fix",
    "commit-lint": "commitlint --from origin/master --to HEAD",
    "start": "dumi dev",
    "docs:build": "dumi build"
  },
  "module": "dist/esm/index.js",
  "typings": "dist/esm/index.d.ts",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@nicecode/changelog": "^0.2.0",
    "@types/node": "^18.7.4",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "conventional-changelog-cli": "^2.2.2",
    "dumi": "^1.1.47",
    "eslint": "^8.22.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "standard": "^17.0.0",
    "typescript": "^4.7.4",
    "yorkie": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run commit-lint"
    }
  },
  "lint-staged": {
    "*.{jsx,js,ts,tsx}": [
      "eslint -c ./.eslintrc --ext .jsx,.js,.ts,.tsx"
    ]
  }
}
