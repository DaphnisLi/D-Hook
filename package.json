{
  "name": "@daphnis/d-hook",
  "version": "2.2.0",
  "description": "Collect commonly used hooks",
  "author": "Daphnis",
  "license": "ISC",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.umd.js"
  },
  "typings": "dist/index.d.ts",
  "sideEffects": [
    "./src/**"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "eslint -c ./.eslintrc --ext .jsx,.js,.ts,.tsx src && stylelint ./src'",
    "lint-fix": "eslint -c ./.eslintrc --ext .jsx,.js,.ts,.tsx src --fix && stylelint ./src --fix",
    "commit-lint": "commitlint --from origin/master --to HEAD",
    "start": "dumi dev",
    "docs:build": "dumi build",
    "build": "rm -rf dist && tsc -p ./tsconfig.build.json && rollup --config",
    "release": "./scripts/release.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run commit-lint"
    }
  },
  "lint-staged": {
    "*.{jsx,js,ts,tsx}": [
      "eslint -c ./.eslintrc --ext .jsx,.js,.ts,.tsx"
    ]
  },
  "files": [
    "package.json",
    "README.md",
    "dist"
  ],
  "keywords": [
    "React",
    "Hook"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@nicecode/changelog": "^0.2.0",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.4",
    "@types/node": "^18.7.4",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.17",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "chalk": "^4.1.2",
    "conventional-changelog-cli": "^2.2.2",
    "dumi": "^1.1.47",
    "eslint": "^8.22.0",
    "eslint-config-standard": "^15.0.1",
    "eslint-config-standard-react": "^10.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^4.0.2",
    "husky": "^8.0.1",
    "inquirer": "^7.3.3",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "rollup": "^2.78.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "semver": "^7.3.7",
    "shelljs": "^0.8.5",
    "standard": "^17.0.0",
    "stylelint": "^14.11.0",
    "stylelint-config-standard": "^28.0.0",
    "typescript": "^4.7.4",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "antd": "^4.22.5",
    "axios": "^0.27.2",
    "immer": "^9.0.15",
    "qs": "^6.11.0",
    "react-router": "^6.3.0",
    "use-immer": "^0.7.0"
  },
  "peerDependencies": {
    "react": "^16",
    "react-dom": "^16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaphnisLi/D-Hook.git"
  },
  "bugs": {
    "url": "https://github.com/DaphnisLi/D-Hook/issues"
  },
  "homepage": "https://github.com/DaphnisLi/D-Hook#readme"
}
